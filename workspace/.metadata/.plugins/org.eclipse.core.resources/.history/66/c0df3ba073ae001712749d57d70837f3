
public class PasswordMaker {
	int MAGIC_NUMBER = 42;
	public final StringBuilder s = new RandomStringGenerator(20, "qwertyuiopasdfghjklzxcvbnm").s;
	String firstName;
	String lastName;
	int age;
	PasswordMaker(String firstName, String lastName, int age){
		this.firstName = firstName;
		this.lastName = lastName;
		this.age = age;
	}
	
	getPassword(){
		String = new s(firstName.substring(s.length() - age % 3) + RandomStringGenerator.)
	}
}
